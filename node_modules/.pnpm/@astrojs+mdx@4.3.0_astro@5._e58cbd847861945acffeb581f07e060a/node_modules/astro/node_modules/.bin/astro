#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/self-pj/my-blog/my-blog/node_modules/.pnpm/astro@5.11.1_@types+node@24_cf89f03042631234711fe62653d32e66/node_modules/astro/node_modules:/mnt/c/self-pj/my-blog/my-blog/node_modules/.pnpm/astro@5.11.1_@types+node@24_cf89f03042631234711fe62653d32e66/node_modules:/mnt/c/self-pj/my-blog/my-blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/self-pj/my-blog/my-blog/node_modules/.pnpm/astro@5.11.1_@types+node@24_cf89f03042631234711fe62653d32e66/node_modules/astro/node_modules:/mnt/c/self-pj/my-blog/my-blog/node_modules/.pnpm/astro@5.11.1_@types+node@24_cf89f03042631234711fe62653d32e66/node_modules:/mnt/c/self-pj/my-blog/my-blog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../astro.js" "$@"
else
  exec node  "$basedir/../../astro.js" "$@"
fi
